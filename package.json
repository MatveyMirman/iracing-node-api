{
  "name": "iracing-node-api",
  "version": "1.0.8",
  "description": "A nodejs wrapper for Iracing's data api.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest --ci",
    "test:coverage": "pnpm test -- --coverage",
    "test:badges": "pnpm run test:coverage  && jest-coverage-badges",
    "build": "rm -rf dist/* && tsc -p tsconfig.mjs.json && tsc -p tsconfig.cjs.json",
    "prepublish": "pnpm build",
    "publish": "",
    "dev": "ts-node src/server.ts"
  },
  "keywords": [
    "iracing",
    "telemetry",
    "iracing-api"
  ],
  "author": "Matvey Mirman",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/MatveyMirman/iracing-node-api"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.2",
    "@types/tough-cookie": "^4.0.5",
    "@types/user-agents": "^1.0.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "morgan": "^1.10.0",
    "nock": "^13.5.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "axios-cookiejar-support": "^5.0.0",
    "crypto-js": "^4.2.0",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "tough-cookie": "^4.1.3",
    "user-agents": "^1.1.460"
  }
}
